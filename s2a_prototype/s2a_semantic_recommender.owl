<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns="http://example.org/s2a#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
>
  <owl:ObjectProperty rdf:about="http://example.org/s2a#hasInterest">
    <rdfs:domain rdf:resource="http://example.org/s2a#Student"/>
    <rdfs:range rdf:resource="http://example.org/s2a#Skill"/>
  </owl:ObjectProperty>
  <Career rdf:about="http://example.org/s2a#DataScientist">
    <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data Scientist</label>
    <requiresSkill>
      <Skill rdf:about="http://example.org/s2a#DataModeling">
        <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data Modeling</label>
      </Skill>
    </requiresSkill>
    <requiresSkill>
      <Skill rdf:about="http://example.org/s2a#MachineLearningSkill">
        <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Machine Learning</label>
      </Skill>
    </requiresSkill>
    <requiresSkill>
      <Skill rdf:about="http://example.org/s2a#DeepLearningSkill">
        <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Deep Learning</label>
      </Skill>
    </requiresSkill>
  </Career>
  <Career rdf:about="http://example.org/s2a#KnowledgeEngineer">
    <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Knowledge Engineer</label>
    <requiresSkill>
      <Skill rdf:about="http://example.org/s2a#KnowledgeGraphs">
        <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Knowledge Graphs</label>
      </Skill>
    </requiresSkill>
    <requiresSkill>
      <Skill rdf:about="http://example.org/s2a#SPARQLSkill">
        <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SPARQL</label>
      </Skill>
    </requiresSkill>
    <requiresSkill rdf:resource="http://example.org/s2a#DataModeling"/>
  </Career>
  <owl:ObjectProperty rdf:about="http://example.org/s2a#requiresSkill">
    <rdfs:domain rdf:resource="http://example.org/s2a#Career"/>
    <rdfs:range rdf:resource="http://example.org/s2a#Skill"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://example.org/s2a#relatedTo">
    <rdfs:domain rdf:resource="http://example.org/s2a#ResearchPaper"/>
    <rdfs:range rdf:resource="http://example.org/s2a#Skill"/>
  </owl:ObjectProperty>
  <Student rdf:about="http://example.org/s2a#Bob">
    <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bob</label>
    <takesCourse>
      <Course rdf:about="http://example.org/s2a#Databases">
        <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Databases</label>
        <credits rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</credits>
        <semester rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</semester>
        <difficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</difficulty>
        <track rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data</track>
        <teachesSkill rdf:resource="http://example.org/s2a#DataModeling"/>
      </Course>
    </takesCourse>
    <hasInterest rdf:resource="http://example.org/s2a#KnowledgeGraphs"/>
    <hasInterest rdf:resource="http://example.org/s2a#SPARQLSkill"/>
    <targetSemester rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</targetSemester>
    <maxCredits rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">12</maxCredits>
    <maxDifficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</maxDifficulty>
    <preferredTrack rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data</preferredTrack>
  </Student>
  <Student rdf:about="http://example.org/s2a#Alice">
    <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Alice</label>
    <takesCourse rdf:resource="http://example.org/s2a#Databases"/>
    <hasInterest rdf:resource="http://example.org/s2a#MachineLearningSkill"/>
    <hasInterest rdf:resource="http://example.org/s2a#DeepLearningSkill"/>
    <targetSemester rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</targetSemester>
    <maxCredits rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">12</maxCredits>
    <maxDifficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</maxDifficulty>
    <preferredTrack rdf:datatype="http://www.w3.org/2001/XMLSchema#string">AI</preferredTrack>
  </Student>
  <Course rdf:about="http://example.org/s2a#DeepLearning">
    <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Deep Learning</label>
    <credits rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</credits>
    <semester rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</semester>
    <difficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</difficulty>
    <track rdf:datatype="http://www.w3.org/2001/XMLSchema#string">AI</track>
    <hasPrerequisite>
      <Course rdf:about="http://example.org/s2a#MachineLearning">
        <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Machine Learning</label>
        <credits rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</credits>
        <semester rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</semester>
        <difficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</difficulty>
        <track rdf:datatype="http://www.w3.org/2001/XMLSchema#string">AI</track>
        <hasPrerequisite rdf:resource="http://example.org/s2a#Databases"/>
        <teachesSkill rdf:resource="http://example.org/s2a#MachineLearningSkill"/>
      </Course>
    </hasPrerequisite>
    <teachesSkill rdf:resource="http://example.org/s2a#DeepLearningSkill"/>
  </Course>
  <Course rdf:about="http://example.org/s2a#SemanticWeb">
    <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Semantic Web &amp; Knowledge Management</label>
    <credits rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</credits>
    <semester rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</semester>
    <difficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</difficulty>
    <track rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data</track>
    <hasPrerequisite rdf:resource="http://example.org/s2a#Databases"/>
    <teachesSkill rdf:resource="http://example.org/s2a#KnowledgeGraphs"/>
    <teachesSkill rdf:resource="http://example.org/s2a#SPARQLSkill"/>
  </Course>
  <owl:DatatypeProperty rdf:about="http://example.org/s2a#semester">
    <rdfs:domain rdf:resource="http://example.org/s2a#Course"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/s2a#difficulty">
    <rdfs:domain rdf:resource="http://example.org/s2a#Course"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://example.org/s2a#teachesSkill">
    <rdfs:domain rdf:resource="http://example.org/s2a#Course"/>
    <rdfs:range rdf:resource="http://example.org/s2a#Skill"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/s2a#maxCredits">
    <rdfs:domain rdf:resource="http://example.org/s2a#Student"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/s2a#targetSemester">
    <rdfs:domain rdf:resource="http://example.org/s2a#Student"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <ResearchPaper rdf:about="http://example.org/s2a#Paper2">
    <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attention Is All You Need (Survey Linkage)</label>
    <relatedTo rdf:resource="http://example.org/s2a#DeepLearningSkill"/>
  </ResearchPaper>
  <ResearchPaper rdf:about="http://example.org/s2a#Paper1">
    <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">RDF and Knowledge Graphs in Practice</label>
    <relatedTo rdf:resource="http://example.org/s2a#KnowledgeGraphs"/>
  </ResearchPaper>
  <owl:DatatypeProperty rdf:about="http://example.org/s2a#credits">
    <rdfs:domain rdf:resource="http://example.org/s2a#Course"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://example.org/s2a#hasPrerequisite">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdfs:domain rdf:resource="http://example.org/s2a#Course"/>
    <rdfs:range rdf:resource="http://example.org/s2a#Course"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/s2a#maxDifficulty">
    <rdfs:domain rdf:resource="http://example.org/s2a#Student"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <ResearchPaper rdf:about="http://example.org/s2a#Paper3">
    <label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SPARQL Query Optimization Overview</label>
    <relatedTo rdf:resource="http://example.org/s2a#SPARQLSkill"/>
  </ResearchPaper>
  <owl:DatatypeProperty rdf:about="http://example.org/s2a#preferredTrack">
    <rdfs:domain rdf:resource="http://example.org/s2a#Student"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://example.org/s2a#takesCourse">
    <rdfs:domain rdf:resource="http://example.org/s2a#Student"/>
    <rdfs:range rdf:resource="http://example.org/s2a#Course"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/s2a#label">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
